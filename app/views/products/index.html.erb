<table>
	<tr>
		<th>Product</th>
		<th>Date</th>
		<th>Price</th>
	</tr>
	<% @products.each do |product| %>
		<tr>
			<td><%= @product.name%></td>
			<td></td>
			<td></td>
		</tr>
	<% end %>
</table>
<h2>Import Products</h2>
<%= form_tag import_products_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>